{
    "name": "jdillenberger/laravel-baseline",
    "description": "Baseline Resources",
    "keywords": [
        "jdillenberger",
        "laravel-baseline"
    ],
    "homepage": "https://github.com/jdillenberger/laravel-baseline",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jan Dillenberger",
            "email": "baseline@jandillenberger.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "mehradsadeghi/laravel-filter-querystring": "^1.1",
        "spatie/laravel-medialibrary": "^11.9",
        "spatie/laravel-multitenancy": "^4.0",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-tags": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Jdillenberger\\LaravelBaseline\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jdillenberger\\LaravelBaseline\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jdillenberger\\LaravelBaseline\\LaravelBaselineServiceProvider"
            ],
            "aliases": {
                "LaravelBaseline": "Jdillenberger\\LaravelBaseline\\LaravelBaselineFacade"
            }
        }
    }
}
